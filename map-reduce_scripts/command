hadoop jar /usr/lib/hadoop-3.2.1/share/hadoop/tools/lib/hadoop-streaming-3.2.1.jar \
-file map1.py    -mapper map1.py \
-file reduce1.py   -reducer reduce1.py \
-input filenames.txt -output output1

